<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         version="3.1"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>URL1</param-name>
        <param-value>http://localhost:8080/page.html</param-value>
    </context-param>

    <context-param>
        <param-name>URL2</param-name>
        <param-value>http://localhost:8080/servlet</param-value>
    </context-param>

    <servlet>
        <servlet-name>GetURLServlet</servlet-name>
        <servlet-class>com.servlets.GetURLServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetURLServlet</servlet-name>
        <url-pattern>/GetURL</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Ccc</servlet-name>
        <servlet-class>com.servlets.Ccc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ccc</servlet-name>
        <url-pattern>/Ccc</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>